{
	"$schema": "https://raw.githubusercontent.com/Madrok/couch-schema/main/cdbschema.json",
	"name": "Person",
	"version": 1,
	"fields": {
		"name": {
			"type": "subschema",
			"schema": "Name",
			"required": true
		},
		"phone": [
			{
				"type": "subschema",
				"schema": "Phone",
				"required": false
			}
		]
	}
}